model Skin {
    id String @id @default(uuid()) @db.Uuid

    name        String
    description String?
    image       String?
    price       Float

    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    deletedAt DateTime?

    userId String @db.Uuid
    user   User   @relation(fields: [userId], references: [id])

    @@map("Skin")
}
